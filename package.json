{
  "name": "rkta-starter-apollo",
  "version": "0.0.1",
  "description": "A boilerplate for internal use",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "build": "next build",
    "check:all": "npm run test && npm run lint && npm run typecheck",
    "commit": "git-cz",
    "deploy": "now --prod",
    "predeploy": "npm run check:all",
    "dev:next": "next -p 8000",
    "dev:watch": "ts-node  -O '{ \"module\": \"CommonJS\"}' scripts/watch-graphql.ts",
    "dev": "run-p dev:**",
    "now:dev": "now dev -p 8000",
    "now-build": "npm run build",
    "prebuild": "npm run check:all",
    "lint": "eslint 'config' 'src' --ext .ts,.tsx",
    "prestart": "npm run build",
    "start": "next start -p 8000",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "updatedeps": "ncu -u && npm update"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "author": "Sergei Droganov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.12.2",
    "@types/cookie": "^0.3.3",
    "@types/detect-node": "^2.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "apollo": "^2.24.0",
    "apollo-server-testing": "^2.10.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-emotion": "^10.0.28",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-emotion": "^10.0.27",
    "jest-transform-graphql": "^2.1.0",
    "node-watch": "^0.6.3",
    "now": "^17.0.4",
    "npm-check-updates": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@emotion/cache": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@rkta/entypo": "^0.0.94",
    "@rkta/hooks": "0.0.94",
    "@rkta/ui": "^0.0.117",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "apollo-utilities": "^1.3.3",
    "cookie": "^0.4.0",
    "create-emotion-server": "^10.0.27",
    "detect-node": "^2.0.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "^9.2.2",
    "next-server": "^9.0.5",
    "next-transpile-modules": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
