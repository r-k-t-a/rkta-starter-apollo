{
  "name": "rkta-starter-apollo",
  "version": "0.0.1",
  "description": "A boilerplate for internal use",
  "main": "index.js",
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "check:all": "npm run test && npm run lint",
    "deploy": "now --target production",
    "predeploy": "npm run check:all",
    "dev": "next -p 8000",
    "now:dev": "now dev -p 8000",
    "build": "next build",
    "now-build": "npm run build",
    "prebuild": "npm run check:all",
    "lint": "eslint 'apollo' 'config' 'pages' 'src' --ext .js,.jsx",
    "prestart": "npm run build",
    "start": "next start -p 8000",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Sergei Droganov",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@types/graphql": "^14.0.5",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "apollo-server-testing": "^2.4.6",
    "babel-eslint": "^10.0.1",
    "babel-plugin-emotion": "^10.0.9",
    "dotenv": "^8.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^24.1.0",
    "jest-emotion": "^10.0.10",
    "jest-transform-graphql": "^2.1.0",
    "now": "latest",
    "prettier": "^1.16.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link": "^1.2.8",
    "apollo-link-context": "^1.0.14",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "next": "latest",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-apollo": "^3.0.0",
    "react-dom": "^16.8.1"
  }
}
